{
    "version": 2,
    "builds": [
        { "src": "cube/**", "use": "@now/static" },
        { "src": "*.html", "use": "@now/static" }
    ],
    "functions": {
        "*.php": {
            "runtime": "now-php"
        }
    },
    "routes": [
        { "src": "/", "dest": "/cube.html" }
    ]
}
